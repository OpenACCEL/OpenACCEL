{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./src/"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["prepost/prepost.js"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
    "template": "prepost/default_prepost",
    "destination": "./doc/"
    }
}