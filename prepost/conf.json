{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["prepost/prepost.js"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
    "template": "prepost/jaguarjs-jsdoc"
    }
}