whiteRice=[n:'white rice;',en:96,ch:0]
brownRice=[n:'brown rice;',en:137,ch:0]
boiledPotatoes=[n:'boiled potatoes;',en:82,ch:0]
bakedPotatoes=[n:'baked potatoes;',en:126,ch:1]
// the 'voedingswaardetabel gave 0 ch ...
frenchFries=[n:'fries;',en:327,ch:10]
pancake=[n:'pancake;',en:210,ch:30]
pasta=[n:'pasta;',en:99,ch:0]
wholemealBread=[n:'wholemeal bread;',en:246,ch:0.5]
whiteBread=[n:'white bread;',en:255, ch:3.5]
pork=[n:'pork;',en:111,ch:40]
chicken=[n:'chicken;',en:100,ch:45]
beef=[n:'beef;', en:187, ch:51]
codfish=[n:'codfish;',en:118,ch: 70]
boiledEgg=[n:'boiled egg;',en:154,ch:320]
friedEgg=[n:'fried egg;',en:235,ch:300]
tempeh=[n:'tempeh;',en:151,ch:0]
mushrooms=[n:'mushrooms;',en:61,ch:8]
cheese=[n:'cheese;',en:317,ch:83]
lettuce=[n:'lettuce;',en:11,ch:0]
greenBeans=[n:'green beans;',en:24,ch:0]
whiteBeans=[n:'white beans;', en:114,ch:0]
aspargus=[n:'aspargus;',en:17,ch:0]
carrots=[n:'carrots;',en:27,ch:0]
whiteSauce=[n:'white sauce;',en:87,ch:2.5]
noSauce=[n:'no sauce;',en:0,ch:0]
// data from http://www.voedingswaardetabel.nl/voedingswaarde/K/
protein=[pork,chicken,beef,codfish,boiledEgg,friedEgg,tempeh,mushrooms,cheese]
veggies=[lettuce,greenBeans,whiteBeans,aspargus,carrots]
sauce=[whiteSauce,noSauce]
addOn=[whiteRice,brownRice,boiledPotatoes,bakedPotatoes,frenchFries,pancake,pasta,wholemealBread,whiteBread]
amountProtein=slider(100,0,1500)
amountVeggies=slider(100,0,1500)
amountSauce=slider(100,0,1500)
amountAddOn=slider(100,0,1500)
whichProtein=slider(0,0,8)
whichVeggies=slider(0,0,4)
whichSauce=slider(0,0,1)
whichAddOn=slider(0,0,8)
myProtein=protein[whichProtein]
myVeggies=veggies[whichVeggies]
mySauce=sauce[whichSauce]
myAddOn=addOn[whichAddOn]
myNameProt=myProtein.n
myNameVeggies=myVeggies.n
myNameAddOn=myAddOn.n
//myNameSauce=mySauce.n
myEn=(amountProtein*myProtein.en+amountVeggies*myVeggies.en+amountAddOn*myAddOn.en+amountSauce*mySauce.en)/100
myCh=(amountProtein*myProtein.ch+amountVeggies*myVeggies.ch+amountAddOn*myAddOn.ch+amountSauce*mySauce.ch)/100
optEn=2200
optCh=0
myOptEn=paretoMin(paretoHor(abs(myEn-optEn)))
myOptCh=paretoMin(paretoVer(myCh))