fill=check(false)
w=slider(1,0,5)
x1=slider(50,0,100)
y1=slider(70,0,100)
x2=slider(70,0,100)
y2=slider(50,0,100)
x3=slider(50,0,100)
y3=slider(50,0,100)
x4=slider(50,0,100)
y4=slider(50,0,100)
fillToken=if(fill,'border','interior')
a=plot([[pCircle],[pSegment],[pQuad],[pBezier]])
pCircle=[plotType:'vector',nPoints:1,x1:x1,x2:x2,y1:y1,y2:y2,fill:fillToken,width:w,type:'circle',col_r:0,fcol_r:255]
pSegment=[plotType:'vector',nPoints:1,tag:'dit is een pijl',end1:'arrow',end2:'arrow',x1:x1,x2:(2*x2-x1),y1:y1,y2:(2*y2-y1),width:w,type:'segment',col_r:0,col_g:0,col_b:80]
pQuad=[plotType:'vector',nPoints:1,x1:x1,x2:x2,x3:x3,x4:x4,y1:y1,y2:y2,y3:y3,y4:y4,width:w,type:'quad',fill:fillToken,fcol_r:0]
pBezier=[plotType:'vector',nPoints:1,tag:'dit is een bezier curve',x1:x1,x2:x2,x3:x3,x4:x4,y1:y1,y2:y2,y3:y3,y4:y4,width:w,type:'bezier',fill:fillToken,fcol_r:0]